@model SportLeague.Models.Team

@{
    ViewData["Title"] = "Division Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Details</h2>
<hr />
<h4>Team name: @Html.DisplayFor(model => model.name)</h4>
<h4>Division Id: @Html.DisplayFor(model => model.divisionId)</h4>
<h5>Players:</h5>
@foreach (var player in Model.Player)
{ <li>@player.name</li>}
<h6>Division: @Model.Division.description</h6>



<p>@Html.ActionLink("Back to list", "Index")</p>
