@model IEnumerable<SportLeague.Models.Team>

@{
    ViewData["Title"] = "Teams Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <h2>Bowling Divisions</h2>
    <h4>@Html.DisplayNameFor(model => model.name)</h4>
    <ul>
        @foreach (var divisionItem in Model)
        {
            <li>@Html.DisplayFor(modelItem => divisionItem.name) | @Html.ActionLink("Details", "Details", new { id = divisionItem.teamId })</li>

        }
    </ul>

<p>@Html.ActionLink("Add new team", "Create") </p>
