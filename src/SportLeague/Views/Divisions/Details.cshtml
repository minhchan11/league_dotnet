@model SportLeague.Models.Division

@{
    ViewData["Title"] = "Division Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Details</h2>
<hr />
<h4>Description:@Html.DisplayFor(model => model.description)</h4>
<h5>Max Team:@Html.DisplayFor(model => model.max_team)</h5>
<h5>Teams:</h5>
    @foreach(var team in Model.Team)
    { <li>@team.name</li>}


<p>@Html.ActionLink("Back to list", "Index")</p>
